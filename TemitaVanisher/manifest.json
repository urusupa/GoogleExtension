{
	"name": "Temita Vanisher",
	"description": "タイトルに てみた と名前がついているものを非表示にします",
	"version": "0.3",
	"web_accessible_resources": [ "image/*", "options.html", "popup.html"],
	"background": { "scripts": ["background.js"] },
	"icons": {
		"16": "image/icon16.png",
		"48": "image/icon48.png",
		"128": "image/icon128.png"
	},
	"manifest_version": 2,
	"permissions": [
		"tabs",
		"*://*.youtube.com/*",
		"*://*.nicovideo.jp/*"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.youtube.com/*",
				"*://*.nicovideo.jp/*",
				"*://*.nicovideo.jp/*"
			],
			"js": ["vanish.js"]
		}
	],
	"page_action": {
		"default_title": "TemitaVanisher",
		"default_icon": {
			"19": "image/icon19.png",
			"38": "image/icon19.png"
		},
		"default_popup": "popup.html"
	},
	"options_page": "options.html"
}

